<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" >
        <shape>
            <size
                android:width="80dp"
                android:height="80dp"
                />
            <solid
                android:color="#282828" />
            <stroke
                android:width="2dp"
                android:color="#282828" />
            <corners
                android:radius="80dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <size
                android:width="80dp"
                android:height="80dp"
                />
            <solid
                android:color="#ACBEC9" />
            <stroke
                android:width="2dp"
                android:color="#000000" />
            <corners
                android:radius="80dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
        </shape>
    </item>
    <item>
        <shape>
            <size
                android:width="80dp"
                android:height="80dp"
                />
            <stroke
                android:width="4dp"
                android:color="#000000" />
            <solid
                android:color="#FFFFFF" />
            <corners
                android:radius="100dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
        </shape>
    </item>
</selector>